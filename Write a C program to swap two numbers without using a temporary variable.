7.// swap two numbers without using a temporary variable
#include <stdio.h>

int main()
{
    int a, b;
    printf("Enter the value of a:");
    scanf("%d",&a);
    printf("Enter the value of b:");
    scanf("%d",&b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("the value of a:%d\n",a);
    printf("the value of b:%d",b);
    

    return 0;
}
