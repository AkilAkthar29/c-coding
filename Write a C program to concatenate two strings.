#include <stdio.h>
#include <string.h>

int main() {
    char str1[100], str2[50];

    // Input two strings
    printf("Enter first string: ");
    gets(str1);
    printf("Enter second string: ");
    gets(str2);

    // Concatenate str2 to str1
    strcat(str1, str2);

    // Print the concatenated result
    printf("Concatenated String: %s\n", str1);
    return 0;
}
